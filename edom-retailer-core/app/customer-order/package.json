{
    "name": "customer-order",
    "version": "0.0.1",
    "description": "Customer Order API application",
    "engines": {
        "node": "14.18.*"
    },
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "scripts": {
        "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
        "build": "ui5 build preload --include-task=generateManifestBundle generateCachebusterInfo && npx cpy-cli ./xs-app.json dist"
    },
    "devDependencies": {
        "@ui5/cli": "2.14.4",
        "@ui5/fs": "2.0.6",
        "@ui5/logger": "2.0.1",
        "@sap/ux-ui5-tooling": "1.4.7",
        "rimraf": "3.0.2",
        "@sap/ux-specification": "latest"
    },
    "cds": {
        "i18n": {
            "folders": [
                "webapp/i18n"
            ]
        }
    }
}
