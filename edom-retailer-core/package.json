                "com.sap."
            ],
            "extension-allowlist": [
                {
                    "for": [
                        "sap.odm.businesspartner.BusinessPartner",
                        "sap.odm.utilities.billingaccount.BillingAccount",
                        "BusinessPartnerService.BusinessPartner",
                        "BillingAccountService.BillingAccount"
                    ],
                    "new-fields": 10
                },
                {
                    "for": [
                        "BusinessPartnerService",
                        "BillingAccountService"
                    ],
                    "new-entities": 3
                }
            ],
            "security": {
                "subscription-scope": "$XSAPPNAME.mtcallback",
                "deployment-scope": "$XSAPPNAME.mtcallback"
            }
        },
        "build": {
            "target": "gen",
            "tasks": [
                {
                    "src": "db",
                    "for": "hana",
                    "options": {
                        "model": [
                            "db",
                            "srv",
                            "srv/external/API_SALES_ORDER_SRV"
                        ]
                    }
                },
                {
                    "src": "srv",
                    "for": "node-cf",
                    "options": {
                        "model": [
                            "db",
                            "srv",
                            "srv/external/API_SALES_ORDER_SRV"
                        ]
                    }
                },
                {
                    "src": ".",
                    "for": "mtx",
                    "dest": "srv"
                }
            ]
        },
        "hana": {
            "syntax": "hdi"
        },
        "odata": {
            "version": "v4",
            "flavor": "x4"
        },
        "folders": {
            "app": " "
        },
        "i18n": {
            "[development]": {
                "default_language": " "
            },
            "[edom-retailer-dev]": {
                "default_language": " "
            },
            "[edom-retailer-test]": {
                "default_language": " "
            }
        }
    },
    "jest": {
        "testEnvironment": "node",
        "testTimeout": 220000,
        "collectCoverage": true,
        "testResultsProcessor": "jest-sonar-reporter",
        "modulePathIgnorePatterns": [
            "/node_modules"
        ],
        "coverageReporters": [
            "cobertura",
            "lcov",
            "text"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/app/",
            "/mock/",
            "/gen/db/",
            "/gen/srv/",
            "/test/",
            "/srv/hooks/upgradeTenants.js"
        ]
    },
    "jest-junit": {
        "outputName": "results.xml",
        "outputDirectory": "./target/backend"
    },
    "jestSonar": {
        "reportFile": "sonar-report.xml",
        "reportPath": "./target/backend",
        "indent": 4,
        "env": {
            "unittest": {
                "reportPath": "./target/backend"
            }
        }
    },
    "ui5": {
        "dependencies": [
            "@sap/ux-ui5-fe-mockserver-middleware",
            "@sap/ux-ui5-tooling"
        ]
    }
}
