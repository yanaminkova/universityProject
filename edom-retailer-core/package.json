{
    "name": "edom-retailer",
    "version": "1.51.1",
    "description": "C4U EDoM Retailer main application",
    "license": "SAP SE",
    "author": "",
    "repository": {
        "type": "git",
        "url": "https://github.wdf.sap.corp/c4u/c4u-cn-edom-retailer"
    },
    "scripts": {
        "start": "cds run",
        "openapi": "cds compile srv --service all -o docs/content/openapi --to openapi",
        "edmx": "cds compile srv --service all -o docs/content/edmx --to edmx-x4",
        "createCustomerOrderAPIDocs": "node scripts/openAPI/createCustomerOrderAPIDocs.js",
        "openAPItranslate": "node scripts/openAPI/i18n2docs.js",
        "build": "cds build --production --clean",
        "lint": "npx eslint .",
        "prettier": "npx prettier --write  .",
        "test": "jest --testPathPattern='/.*/backend-[a-z]+/.*.js' --reporters='jest-junit' --reporters='default' --coverage=true --coverageDirectory='target/backend/' ",
        "ci-lint": "npx eslint -o ./eslint.xml -f checkstyle .",
        "newman-local": "newman run test/postman/edom_retailer_test.postman_collection.json --environment test/postman/localhost.postman_environment.json",
        "newman-dev": "newman run test/postman/edom_retailer_test.postman_collection.json --environment test/postman/c4uconsumerdevaws.postman_environment.json",
        "newman-test": "newman run test/postman/edom_retailer_test.postman_collection.json --environment test/postman/c4uconsumertestaws.postman_environment.json --globals test/postman/edom_retailer_test.postman_globals.json",
        "karma-start": "karma start karma.conf.js",
        "karma-start-ci": "wait-on http://localhost:8080/fiori.html && karma start karma-ci.conf.js",
        "start-ui-mock": "ui5 serve --config app/ui5-cap.yaml",
        "ci-it-frontend": "concurrently -k -s first \"npm run start-ui-mock\"  \"npm run karma-start-ci\"",
        "prepare": "node srv/hooks/huskyPrepare.js"
    },
